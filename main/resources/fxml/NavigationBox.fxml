<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="navigationItemsBox" prefHeight="302.0" prefWidth="112.0" styleClass="navigation-window" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cbmwebdevelopment.main.Navigation">
   <children>
      <ImageView fitHeight="125.0" fitWidth="112.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/auction.jpg" />
         </image>
      </ImageView>
      <Label styleClass="navigation-window__section-heading" text="Auctioneer">
         <VBox.margin>
            <Insets left="5.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="auctionDashboardButton" mnemonicParsing="false" onAction="#auctionDashboardAction" styleClass="navigation-window__button" text="Auctions" />
      <Button fx:id="bidsListButton" mnemonicParsing="false" onAction="#viewBidsListAction" styleClass="navigation-window__button" text="Bids" />
      <Button fx:id="bidderListButton" mnemonicParsing="false" onAction="#viewBidderListAction" styleClass="navigation-window__button" text="Bidders" />
      <Button fx:id="itemListButton" mnemonicParsing="false" onAction="#viewAuctionItemsListAction" styleClass="navigation-window__button" text="Items" />
   </children>
</VBox>
